{
  "name": "trafficflowapi",
  "version": "1.0.0",
  "description": "Provides a bunch of useful information related to traffic flow, weather, and road types.",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "env NODE_ENV=development nodemon bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "production": "npm run build && env NODE_ENV=production node bin/production",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azadyasar/TrafficFlowAPI.git"
  },
  "keywords": [
    "traffic",
    "flow",
    "api",
    "weather",
    "road"
  ],
  "author": "Azad-Emre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/azadyasar/TrafficFlowAPI/issues"
  },
  "homepage": "https://github.com/azadyasar/TrafficFlowAPI#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jest": "^24.5.0",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0"
  }
}
